  "repository": "reggiepangilinan/modus-ui",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "plop": "plop && npm run prettier",
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "npm run lint && rollup -c -w",
    "lint": "eslint --fix --ext js,ts,tsx src",
    "prettier": "prettier --write src/",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "storybook": "npm run prettier && npm run lint && start-storybook -p 6006 -s ./public,./static --docs",
    "build-storybook": "build-storybook -s ./public,./static --docs"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {},
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-select": "^3.0.4",
    "chrono-node": "^2.1.7",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/runtime": "7.11.0",
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-url": "5.0.0",
